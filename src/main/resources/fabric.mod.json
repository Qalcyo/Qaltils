  "authors": [
    "Qalcyo"
  ],
  "contact": {
    "issues": "https://github.com/Qalcyo/Rysm/issues/",
    "sources": "https://github.com/Qalcyo/Rysm/"
  },
  "license": "GNU Lesser General Public License version 3 (LGPLv3)",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "xyz.qalcyo.rysm.Rysm"
      }
    ],
    "preLaunch": [
      "gg.essential.loader.stage0.EssentialSetupPreLaunch"
